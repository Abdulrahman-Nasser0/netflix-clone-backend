    "paths": {
        "/api/add": {
            "post": {
                "tags": [
                    "Favorites"
                ],
                "summary": "Add a media item to favorites",
                "description": "Adds or updates a media item in the authenticated user's favorites",
                "operationId": "58a945f688123421d8743bb3f5e62cd0",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "tmdb_id",
                                    "media_type"
                                ],
                                "properties": {
                                    "tmdb_id": {
                                        "type": "string",
                                        "example": "12345"
                                    },
                                    "media_type": {
                                        "type": "string",
                                        "example": "movie"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Added to favorites successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Added to favorites successfully"
                                        },
                                        "favorite": {
                                            "properties": {
                                                "tmdb_id": {
                                                    "type": "string",
                                                    "example": "12345"
                                                },
                                                "media_type": {
                                                    "type": "string",
                                                    "example": "movie"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Unauthenticated"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Validation failed"
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Failed to add to favorites"
                                        },
                                        "error": {
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/retrieve": {
